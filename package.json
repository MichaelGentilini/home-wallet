  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "generate-password": "^1.4.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.6",
    "nodemailer": "^6.3.0",
    "reactstrap": "^8.0.1",
    "uuid": "^3.3.2"
